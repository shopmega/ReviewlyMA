{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9002",
    "dev:turbopack": "next dev --turbopack -p 9002",
    "seed:catalog": "tsx src/scripts/seed-app-catalog.ts",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:chunk": "node scripts/vitest-deterministic.mjs chunk",
    "test:unit:all": "node scripts/vitest-deterministic.mjs all",
    "test:all-unit": "npm run test:unit:all",
    "test:unit:critical": "vitest run -c vitest.critical.config.ts",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:critical": "playwright test -c playwright.critical.config.ts",
    "test:e2e:prod:critical": "playwright test -c playwright.prod-critical.config.ts",
    "test:e2e:prod:salaries": "playwright test -c playwright.salaries-prod-critical.config.ts",
    "test:e2e:prod:userpro": "playwright test -c playwright.prod-userpro.config.ts",
    "test:ci:critical": "npm run test:unit:critical && npm run test:e2e:critical",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "validate:prod": "powershell -ExecutionPolicy Bypass -File scripts/run-prod-validation.ps1",
    "validate:prod:salaries": "powershell -ExecutionPolicy Bypass -File scripts/run-prod-salaries-validation.ps1",
    "validate:prod:userpro": "powershell -ExecutionPolicy Bypass -File scripts/run-userpro-validation.ps1",
    "check:seo-ia": "node scripts/check-seo-ia.mjs"
  },
  "dependencies": {
    "@genkit-ai/google-genai": "^1.20.0",
    "@genkit-ai/next": "^1.20.0",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "embla-carousel-autoplay": "^8.1.7",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.9.1",
    "genkit": "^1.20.0",
    "isomorphic-dompurify": "^2.30.0",
    "lucide-react": "^0.475.0",
    "next": "15.5.12",
    "next-themes": "^0.4.0",
    "papaparse": "^5.5.3",
    "react": "^19.2.1",
    "react-day-picker": "^9.11.3",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "redis": "^4.7.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.57.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/papaparse": "^5.5.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "genkit-cli": "^1.20.0",
    "jsdom": "^27.4.0",
    "patch-package": "^8.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "5.9.3",
    "vitest": "^2.1.9"
  }
}
